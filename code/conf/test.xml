<store>
    <name>test</name>
    <persistence>bdb</persistence>
    <description>Distribuert datalager for testing av DBUpgradinator</description>
    <owners>vegardbb@stud.ntnu.no, sveinbra@ntnu.no</owners>
    <routing-strategy>consistent-routing</routing-strategy>
    <routing>client</routing>
    <hinted-handoff-strategy>consistent-handoff</hinted-handoff-strategy>
    <replication-factor>3</replication-factor>
    <required-reads>2</required-reads>
    <required-writes>2</required-writes>
    <key-serializer>
      <type>string</type>
    </key-serializer>
    <value-serializer>
      <type>string</type>
      <schema-info>utf8</schema-info>
    </value-serializer>
</store>